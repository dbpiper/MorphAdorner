#!/bin/sh
SCRIPTPATH=$(dirname "$0")
CLASSPATH="$SCRIPTPATH":"$SCRIPTPATH"/bin/:"$SCRIPTPATH"/dist/\*:"$SCRIPTPATH"/lib/\*
java -Xmx6144m -cp "$CLASSPATH" \
	edu.northwestern.at.morphadorner.MorphAdorner \
	-d "$SCRIPTPATH"/morphadorner.properties \
	-p "$SCRIPTPATH"/eme.properties \
	-l "$SCRIPTPATH"/data/emelexicon.lex \
	-t "$SCRIPTPATH"/data/emetransmat.mat \
	-u "$SCRIPTPATH"/data/emesuffixlexicon.lex \
	-a "$SCRIPTPATH"/data/ememergedspellingpairs.tab \
	-s "$SCRIPTPATH"/data/standardspellings.txt \
	-w "$SCRIPTPATH"/data/spellingsbywordclass.txt \
	-o "$1" \
	"$2" "$3" "$4" "$5" "$6" "$7" "$8" "$9"
